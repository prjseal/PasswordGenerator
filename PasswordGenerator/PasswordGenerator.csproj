<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>PasswordGenerator</PackageId>
    <Version>2.0.0</Version>
    <Authors>Paul Seal</Authors>
    <Company>Open Source</Company>
    <Copyright>Copyright 2017</Copyright>
    <PackageLicenseUrl>https://github.com/prjseal/PasswordGenerator/blob/master/License.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/prjseal/PasswordGenerator/</PackageProjectUrl>
    <PackageIconUrl>https://github.com/prjseal/PasswordGenerator/master/pg_logo.png</PackageIconUrl>
    <Description>A .Net Standard library which generates random passwords with different settings to meet the OWASP requirements</Description>
    <PackageTags>Password,Generator,OWASP,Security,Random,Special,Characters</PackageTags>
  </PropertyGroup>

</Project>